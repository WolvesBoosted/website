{
    "env":
    {
        "browser": true,
        "node": true,
        "es6": true
    },
    "plugins": [
        "react"
    ],
    "extends": ["eslint:recommended", "google"],
    "parser": "babel-eslint",
    "parserOptions":
    {
        "ecmaVersion": 6,
        "sourceType": "module",
        "ecmaFeatures":
        {
            "jsx": true,
            "modules": true,
            "experimentalObjectRestSpread": true
        }
    },
    "rules":
    {
        "no-unused-vars": 0,
        "require-jsdoc": 0,
        "no-undef": 0,
        "jsx-a11y/alt-text": 0,
        "array-callback-return":0,
        "no-restricted-globals": 1,
        "no-unused-expressions": 1,
        "max-len": 1
    }
}